{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": "angular",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://abthecuat.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}